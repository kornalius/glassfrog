form#editor-modules-search(role='search')
  .form-group
    .input-group
      input.form-control(type='text', placeholder='Search', ng-model="search")
      span.input-group-btn
        button.btn.btn-default(type="button")
          span.cic.cic-search

#editor-modules-list(ng-repeat="m in list() | filter: search")

  include includes/module-template
